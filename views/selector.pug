extends layout

block content
  h1= title
  p Welcome to #{title}. My self Kiran Gundapuneni
  - var image_names = ["Aquarius.jpg","cancer.jpg","Gemini.jpg","Leo.jpg","virgo.jpg"]
  - for(let i=0; i<3; i++){
    - var j=Math.floor(Math.random()*5);
    - var img_j=image_names[j];
    image(height="300" width="300" src=`\\images\\${img_j}` alt=`${img_j}` )
  - }